{
  "name": "@cds/angular-workspace",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "build": "npm-run-all clean build:lib",
    "build:lib": "npm run generate:core-ng-modules && ng build cds-angular --configuration=production",
    "clean": "cd ./projects/cds-angular && git clean -dfX",
    "generate:core-ng-modules": "node ../../scripts/core-ng-module-generator && prettier --write ./projects/cds-angular/",
    "test": "ng test cds-angular --watch false  --source-map=false",
    "lint": "eslint -c .eslintrc.js \"projects/**/*.ts\"",
    "lint:fix": "eslint --fix -c .eslintrc.js \"projects/**/*.ts\""
  },
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@cds/angular": "file:./dist",
    "@cds/core": "file:../core/dist/core",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.0",
    "@angular/cli": "~13.3.0",
    "@angular/compiler-cli": "~13.3.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "eslint-plugin-unused-imports": "2.0.0",
    "jasmine-core": "4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^13.0.0",
    "typescript": "~4.6.2"
  }
}
